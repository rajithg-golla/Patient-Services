<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MVN_Is_Upcoming__c</fullName>
    <description>Formula which marks ccheckbox as TRUE if the Confirmed Appt. Date is 1 Day away. Created for Evkeeza by Jon Prashaw.</description>
    <externalId>false</externalId>
    <formula>IF((RecordType.DeveloperName = &quot;MVN_Evkeeza&quot; &amp;&amp;   MVN_Confirmed_Appt_Date__c   = ( TODAY() + 1)), TRUE, FALSE)</formula>
    <label>Is Upcoming</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
