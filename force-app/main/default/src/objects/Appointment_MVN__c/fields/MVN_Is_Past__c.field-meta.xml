<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MVN_Is_Past__c</fullName>
    <description>Formula field which is set to True if the Confirmed Apppointment Date is 1 day prior to today. Created for Evkeeza by Jon Prashaw.</description>
    <externalId>false</externalId>
    <formula>IF((RecordType.DeveloperName = &quot;MVN_Evkeeza&quot; &amp;&amp;   MVN_Confirmed_Appt_Date__c    = ( TODAY() - 1)), TRUE, FALSE)</formula>
    <label>Is Past</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
