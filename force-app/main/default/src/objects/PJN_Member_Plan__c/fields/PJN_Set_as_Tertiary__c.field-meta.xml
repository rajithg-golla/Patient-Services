<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PJN_Set_as_Tertiary__c</fullName>
    <description>Indicator to show if this should be set as the tertiary member plan</description>
    <externalId>false</externalId>
    <formula>IF( AND(PJN_Set_as_Active__c, TEXT(PJN_Coverage_Order__c) == &apos;Tertiary&apos; ) , true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Set as Tertiary</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
