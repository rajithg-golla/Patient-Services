<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PJN_Closed_Date_Formula__c</fullName>
    <description>Returns the date portion of the standard Closed Date date/time field.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(ClosedDate)),
   DATEVALUE(ClosedDate),
   null
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Closed Date Formula</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
