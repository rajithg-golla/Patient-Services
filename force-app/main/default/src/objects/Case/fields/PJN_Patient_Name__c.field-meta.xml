<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PJN_Patient_Name__c</fullName>
    <description>Returns the first and last name of the patient associated to the care plan or activity.</description>
    <externalId>false</externalId>
    <formula>HYPERLINK(&quot;/&quot;+AccountId, Account.FirstName + &quot; &quot; + Account.LastName , &quot;_self&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Patient Name</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
