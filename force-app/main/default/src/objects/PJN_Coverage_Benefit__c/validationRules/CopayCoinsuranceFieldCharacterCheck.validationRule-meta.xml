<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CopayCoinsuranceFieldCharacterCheck</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( MVN_Drug_Copay_Coinsurance__c ,&quot;^\\$?\\d*%?$&quot;))</errorConditionFormula>
    <errorDisplayField>MVN_Drug_Copay_Coinsurance__c</errorDisplayField>
    <errorMessage>Copay/Coinsurance may only contain 0-9, %, or $ characters</errorMessage>
</ValidationRule>
