<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingOwnerRules>
        <fullName>DEDSI_Goal_Sharing_MVN</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Shares all care plan goal records owned by users in the DEDSI role hierarchy to all other DEDSI users.</description>
        <label>DEDSI Goal Sharing</label>
        <sharedTo>
            <roleAndSubordinates>DEDSI_MVN</roleAndSubordinates>
        </sharedTo>
        <sharedFrom>
            <roleAndSubordinates>DEDSI_MVN</roleAndSubordinates>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>DEDSI_Nurse_Educators_Group_MVN</fullName>
        <accessLevel>Edit</accessLevel>
        <label>DEDSI Nurse Educators</label>
        <sharedTo>
            <group>MVN_DEDSI_Nurse_Educators</group>
        </sharedTo>
        <sharedFrom>
            <group>MVN_DEDSI_Nurse_Educators</group>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
