<aura:component implements="c:PJN_LightningOutIntf">
    <!-- ATTRIBUTES -->
    <aura:attribute name="caseWrapper" type="PJN_CarePlanCaseWrapper" />
    <aura:attribute name="activityFieldSetting" type="PJN_Care_Plan_Timeline_Case_Field__mdt" />
    <aura:attribute name="fieldValue" type="String" />

    <!-- HANDLERS -->
    <aura:handler name="init" value="{!this}" action="{!c.onInit}"/>

    <!-- HELPERS -->
    <c:PJN_ToastErrorHandler aura:id="toastErrorHandler" />

    <!-- BODY -->
    <p class="slds-m-horizontal_xx-small">
        {!v.fieldValue}
    </p>
</aura:component>